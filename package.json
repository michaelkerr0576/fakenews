{
  "name": "fakenews",
  "version": "1.0.0",
  "description": "Fake News built with MERN Stack",
  "main": "app.js",
  "scripts": {
    "start": "node server/server.js",
    "server": "nodemon server/server.js",
    "client": "npm start --prefix client",
    "client-install": "npm install --prefix client",
    "client-build": "npm run build --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "author": "Michael Kerr",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.19.0",
    "chokidar": "^3.4.1",
    "concurrently": "^5.2.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.3",
    "mongoose": "^5.9.25",
    "save": "^2.4.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.4"
  }
}
